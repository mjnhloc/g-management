{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"type": "object",
	"additionalProperties": false,
    "required": ["member"],
	"properties": {
		"member": {
			"type": "object",
			"additionalProperties": false,
			"properties": {
                "name": {
                    "type": "string"
                },
                "email": {
                    "type": ["string", "null"]
                },
                "phone": {
                    "type": "string"
                },
				"date_of_birth": {
					"type": ["string", "null"],
                    "format": "date"
				},
                "is_active": {
                    "type": "boolean"
                }
			}
		}
	}
}
